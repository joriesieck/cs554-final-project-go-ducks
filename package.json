	"bugs": {
		"url": "https://github.com/joriesieck/cs554-final-project-go-ducks/issues"
	},
	"homepage": "https://github.com/joriesieck/cs554-final-project-go-ducks#readme",
	"dependencies": {
		"concurrently": "^6.4.0"
	}
}
