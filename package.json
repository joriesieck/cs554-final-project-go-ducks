{
	"name": "jeopardy-trainer",
	"version": "1.0.0",
	"description": "CS554 final project",
	"scripts": {
		"client-install": "cd ./client && npm install",
		"server-install": "cd ./server && npm install",
		"install-dependencies": "npm install && concurrently \"npm run client-install\" \"npm run server-install\"",
		"start-client": "cd client && npm run dev",
		"start-server": "cd server && npm start",
		"start": "concurrently \"npm run start-client\" \"npm run start-server\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/joriesieck/cs554-final-project-go-ducks.git"
	},
